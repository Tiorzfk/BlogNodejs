<% include ../../partials/headeradmin %>
    <div class="middle-box text-center loginscreen  animated fadeInDown">
        <div>
            <div>

                <h1 class="logo-name"><img src="/backend/img/logo.png" style="width:150px;height:160px"></h1>

            </div><br>
            <font color="#DFEFFF">
            <h3>Login Admin Form</h3>
            <br>
            <% for(var i in messages) { %>
                <div class="alert alert-danger alert-dismissable">
                    <button aria-hidden="true" data-dismiss="alert" class="close" type="button">Ã—</button>
                        <li><%= messages[i] %></li>
                 </div>
            <% } %>
            <form class="m-t" role="form" action="" method="post">
                <div class="form-group">
                    <input type="email" name="email" class="form-control" placeholder="Username" required="" style="color:#000">
                </div>
                <div class="form-group">
                    <input type="password" name="password" class="form-control" placeholder="Password" required="" style="color:#000">
                </div>
                <button type="submit" class="btn btn-primary block full-width m-b">Login</button>

            </form>
        </div>
    </div>
    <script src="/backend/js/jquery-2.1.1.js"></script>
    <script src="/backend/js/bootstrap.min.js"></script>
